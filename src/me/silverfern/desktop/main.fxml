<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.silverfern.desktop.Controller">
    <stylesheets>
        <URL value="@/assets/base.css" />
    </stylesheets>
    <Button layoutX="467.0" mnemonicParsing="false" onAction="#closeApp" text="X" textAlignment="CENTER" GridPane.columnIndex="4">
        <font>
            <Font name="Arial" size="12.0" />
        </font>
        <styleClass>
            <String fx:value="window-buttons" />
            <String fx:value="close-button" />
        </styleClass>
    </Button>
</AnchorPane>
